{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Removed Trees Table",
    "description": "Removed Trees table for the Ground Pot Data Collection Tally Sheet #10",
    "type": "object",
    "properties": {
        "removed_treeNumber": {
            "type": "number",
            "minimum": 1,
            "maximum": 9999,
            "required": true
        },
        "removed_reason": {
            "type": "string",
            "enum": [" ", "C", "B", "F", "U", "O", "N"],
            "default": " ",
            "disallow": [" "],
            "required": true
        }
    }
}
